{% extends 'layout.twig' %}

{% block body %}
 
<div class="row mt-5">
  <div class="col-md-6 m-auto">


<h3 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i>There are {{ numnodes}} worker nodes available</h3>
  <table class="table table-bordered table-hover ">
  <thead>
  <tr>
    <th>URL</th>
    <th>Report Link</th>
  </tr>
  </thead>
  <tbody>
  {% for url in result %}
  <tr>
    <th>{{url.url}}</th>
    <th><a href={{"/item/%s"|format(url.id)}}>Report</a></th>
  </tr>
  {% endfor %}

  </tbody>

  </table>

  </div>
</div>




{% endblock %}
